<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pentoryall.membership.mapper.MembershipMapper">

    <select id="createMembership" resultType="com.pentoryall.membership.dto.MembershipDTO">
        SELECT code,
               user_code,
               name,
               introductio,
               price,
               color,
               is_active,
               is_deleted,
               create_date,
               update_date,
               delete_date
